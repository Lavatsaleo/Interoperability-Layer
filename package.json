{
  "name": "sequelize-example",
  "version": "1.0.0",
  "description": "A simple sequelize example",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets es2015,stage-3",
    "build": "babel lib -d dist",
    "serve": "node dist/index.js",
    "test": "lab -t 100 -S -c -v -T  ./node_modules/lab-babel -I __core-js_shared__ ./test/",
    "cover": "lab -S -T ./node_modules/lab-babel -I __core-js_shared__ -r html -o ./test/coverage.html ./test/"
  },
  "author": "Teddy Odhiambo",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  },
  "dependencies": {
    "blipp": "^2.3.0",
    "boom": "^5.2.0",
    "code": "^4.1.0",
    "good": "^7.2.0",
    "good-console": "^6.4.0",
    "good-squeeze": "^5.0.2",
    "hapi": "^16.5.0",
    "inert": "^4.2.1",
    "lab": "^14.1.1",
    "lab-babel": "^1.1.1",
    "labbable": "^2.1.1",
    "lout": "^10.0.2",
    "mysql2": "^1.4.0",
    "sequelize": "^4.4.2",
    "sqlite3": "^3.1.8",
    "vision": "^4.1.1"
  },
  "babel": {
    "presets": [
      "stage-3",
      "es2015"
    ]
  }
}
